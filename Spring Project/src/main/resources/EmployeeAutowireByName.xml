<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="employee"
	class="com.spring.ioc.beanfactory.autowiring.byname.Employee"
	autowire="byName">
		<property name="emplyoeeId" value="1024578" />
		<property name="emplyoeeName" value="Regu" />
	</bean>
	<bean id="address"
		class="com.spring.ioc.beanfactory.autowiring.byname.Address">
		<property name="address1" value="1st Cross" />
		<property name="address2" value="Electronic City, Bangalore" />
	</bean>
</beans>